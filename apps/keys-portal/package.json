{
  "name": "@onecore/keys-portal",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "nginx.conf"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "wait-on http://localhost:5090/swagger && wait-on http://localhost:5010/swagger && pnpm run generate-api-types && pnpm run generate-api-types:core && vite",
    "dev:init": "pnpm run prepare:dotenv",
    "format": "prettier --write .",
    "generate-api-types": "npx openapi-typescript http://localhost:5090/swagger.json -o ./src/services/api/generated/api-types.ts",
    "generate-api-types:core": "npx openapi-typescript http://localhost:5010/swagger.json -o ./src/services/api/core/generated/api-types.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare:dotenv": "[ -f .env ] || cp .env.local.example .env",
    "prettier": "prettier --check ./src",
    "start": "vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@onecore/types": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.62.7",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.344.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.13.8",
    "react": "^18.3.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^6.22.2",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.7.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "lovable-tagger": "^1.1.9",
    "openapi-typescript": "^6.7.6",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^6.3.5",
    "wait-on": "^8.0.2"
  }
}
