{
  "name": "@onecore/property-tree",
  "version": "1.4.22",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "vite build",
    "dev": "wait-on http://localhost:5050/swagger && wait-on http://localhost:5010/swagger && pnpm run generate-api-types && pnpm run generate-api-types:core && vite",
    "dev:init": "pnpm run prepare:dotenv",
    "format": "prettier --write .",
    "generate-api-types": "npx openapi-typescript http://localhost:5050/swagger.json -o ./src/services/api/generated/api-types.ts",
    "generate-api-types:core": "npx openapi-typescript http://localhost:5010/swagger.json -o ./src/services/api/core/generated/api-types.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare:dotenv": "[ -f .env ] || cp .env.local.example .env",
    "prettier": "prettier --check ./src",
    "start": "vite",
    "start:server": "node --import tsx src/server/api.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deck.gl/core": "^9.0.38",
    "@deck.gl/layers": "^9.0.38",
    "@deck.gl/react": "^9.0.38",
    "@headlessui/react": "^1.7.18",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@tanstack/react-query": "^5.62.7",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "deck.gl": "^9.0.38",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.0.8",
    "http-proxy-middleware": "^3.0.3",
    "lucide-react": "^0.344.0",
    "maplibre-gl": "5.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-map-gl": "^8.0.0",
    "react-router-dom": "^6.22.2",
    "tailwind-merge": "^2.5.5",
    "ts-pattern": "^5.7.0",
    "vite": "^6.2.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.31.0",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^6.7.6",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "wait-on": "^8.0.2"
  }
}
