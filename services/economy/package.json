{
  "name": "@onecore/economy",
  "version": "1.4.22",
  "description": "ONECore Economy Service",
  "main": "./src/index.ts",
  "packageManager": "pnpm@10.6.1",
  "private": "true",
  "scripts": {
    "test": "jest",
    "build": "holidays2json --pick SE --min && tsc",
    "test:watch": "TZ=UTC jest --watch",
    "start": "pnpm run migrate:up && node -r dotenv/config build/index",
    "dev": "pnpm run migrate:up && nodemon -e ts,js --exec ts-node src/index ",
    "dev:init": "pnpm run prepare:dotenv",
    "lint": "eslint src/",
    "migrate:make": "knex migrate:make",
    "migrate:up": "knex migrate:latest --env dev",
    "migrate:down": "knex migrate:down --env dev",
    "prettier": "prettier --check ./src",
    "seed": "knex seed:run --env dev",
    "ts:watch": "tsc --watch --noEmit",
    "test:ci": "NODE_ENV=test-ci jest --silent",
    "ts:ci": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@iteam/sanity-check": "^1.0.1",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "2.15.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "12.0.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-pino-logger": "3.0.4",
    "@types/node": "^20.11.5",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@types/supertest": "2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui": "^3.52.4",
    "eslint": "^9.31.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@iteam/config": "^12.1.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@urql/core": "^5.1.0",
    "axios": "^1.7.2",
    "axios-logger": "^2.8.0",
    "date-holidays": "^3.23.12",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^5.2.0",
    "graphql-request": "^7.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-pino-logger": "^4.0.0",
    "koa2-swagger-ui": "^5.10.0",
    "@onecore/types": "workspace:*",
    "@onecore/utilities": "workspace:*",
    "pino": "^9.1.0",
    "pino-elasticsearch": "^8.0.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.6.2",
    "ssh2-sftp-client": "^12.0.1",
    "swagger-jsdoc": "^6.2.8",
    "tedious": "^18.6.1"
  }
}
