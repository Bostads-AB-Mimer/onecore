{
  "name": "@onecore/keys",
  "private": true,
  "version": "0.1.0",
  "description": "MIT",
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env DOTENV_CONFIG_PATH=.env pnpm run migrate:up && nodemon -e ts,js --exec ts-node -r dotenv/config src/index",
    "dev:init": "pnpm run prepare:dotenv",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate:down": "knex migrate:down --env dev",
    "migrate:make": "knex migrate:make",
    "migrate:seed": "knex seed:run --env dev",
    "migrate:up": "knex migrate:latest --env dev",
    "prepare:dotenv": "[ -f .env ] || cp .env.template .env",
    "start": "pnpm run migrate:up && node -r dotenv/config build/index",
    "test": "DOTENV_CONFIG_PATH=.env.test node -r dotenv-config ../../node_modules/.bin/jest --config jest.config.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@koa/cors": "5.0.0",
    "@koa/router": "14.0.0",
    "@onecore/types": "workspace:*",
    "@onecore/utilities": "workspace:*",
    "dotenv": "17.2.2",
    "knex": "3.1.0",
    "koa": "3.0.1",
    "koa-bodyparser": "4.4.1",
    "koa-pino-logger": "4.0.0",
    "mssql": "11.0.1",
    "pino": "9.9.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.6",
    "@types/node": "^24.5.2",
    "cross-env": "10.0.0",
    "nodemon": "3.1.10",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
