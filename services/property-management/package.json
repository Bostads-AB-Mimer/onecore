{
  "name": "@onecore/property-management",
  "version": "1.0.59",
  "private": "true",
  "description": "",
  "license": "MIT",
  "author": "",
  "packageManager": "npm",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "npm run migrate:up && nodemon -e ts,js --exec ts-node src/index ",
    "dev:init": "npm run prepare:dotenv",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate:down": "knex migrate:down --env dev",
    "migrate:make": "knex migrate:make",
    "migrate:seed": "knex seed:run --env dev",
    "migrate:up": "knex migrate:latest --env dev",
    "prepare:dotenv": "[ -f .env ] || cp .env.template .env",
    "prettier": "prettier --check ./src",
    "start": "npm run migrate:up && node -r dotenv/config index",
    "test": "jest",
    "test:ci": "jest --silent",
    "ts:watch": "tsc --watch --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iteam/config": "^12.1.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui": "^3.52.4",
    "axios": "^1.6.5",
    "dotenv": "^16.3.2",
    "easy-soap-request": "^5.6.1",
    "fast-xml-parser": "^4.3.5",
    "http-errors": "^2.0.0",
    "knex": "^2.5.1",
    "koa": "^2.15.0",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-pino-logger": "^4.0.0",
    "koa2-swagger-ui": "^5.10.0",
    "onecore-types": "^3.11.2",
    "onecore-utilities": "^1.2.0",
    "swagger-jsdoc": "^6.2.8",
    "tedious": "^16.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/easy-soap-request": "^4.1.4",
    "@types/jest": "^29.5.11",
    "@types/koa": "2.15.0",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-pino-logger": "3.0.1",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-node": "11.1.0",
    "fishery": "^2.2.2",
    "jest": "29.6.4",
    "nodemon": "3.0.1",
    "prettier": "^3.6.2",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "8.38.0"
  }
}
