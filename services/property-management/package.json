{
  "name": "@onecore/property-management",
  "version": "1.5.0",
  "private": "true",
  "description": "",
  "license": "MIT",
  "author": "",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm migrate:up && nodemon -e ts,js --exec ts-node src/index ",
    "dev:init": "pnpm prepare:dotenv",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate:down": "knex migrate:down --env dev",
    "migrate:make": "knex migrate:make",
    "migrate:seed": "knex seed:run --env dev",
    "migrate:up": "knex migrate:latest --env dev",
    "prepare:dotenv": "[ -f .env ] || cp .env.template .env",
    "prettier": "prettier --check ./src",
    "start": "pnpm migrate:up && node -r dotenv/config build/index",
    "test": "jest",
    "test:ci": "jest --silent",
    "ts:watch": "tsc --watch --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iteam/config": "^12.1.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui": "^3.52.4",
    "axios": "^1.9.0",
    "dotenv": "^16.4.5",
    "easy-soap-request": "^5.6.1",
    "fast-xml-parser": "^4.4.0",
    "http-errors": "^2.0.0",
    "knex": "^3.1.0",
    "koa": "^2.16.2",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-pino-logger": "^4.0.0",
    "koa2-swagger-ui": "^5.11.0",
    "swagger-jsdoc": "^6.2.8",
    "tedious": "^18.6.1",
    "zod": "^3.25.67",
    "@onecore/types": "workspace:*",
    "@onecore/utilities": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/easy-soap-request": "^5.6.0",
    "@types/http-errors": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-pino-logger": "3.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-node": "^11.1.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "8.38.0"
  }
}
