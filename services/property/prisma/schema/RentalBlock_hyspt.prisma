model RentalBlock {
  id               String    @id(map: "pkhyspt") @map("keyhyspt") @db.Char(15)
  propertyObjectId String    @map("keycmobj") @db.Char(15)
  blockReasonId    String    @map("keyhyspa") @db.Char(15)
  projectId        String?   @map("keyprprj") @db.Char(15)
  fromDate         DateTime  @map("fdate") @db.DateTime
  toDate           DateTime? @map("tdate") @db.DateTime
  amount           Float?    @map("amount") @db.Money
  timestamp        String    @map("timestamp") @db.Char(10)

  // Relations
  blockReason    BlockReason    @relation(fields: [blockReasonId], references: [id], onUpdate: NoAction, map: "fkhysptkeyhyspa")
  propertyObject PropertyObject @relation(fields: [propertyObjectId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fkhysptkeycmobj")

  @@index([blockReasonId], map: "fkhyspthyspa")
  @@index([propertyObjectId], map: "fkhysptcmobj")
  @@index([fromDate], map: "inhyspt_fdate")
  @@index([toDate], map: "inhyspt_tdate")
  @@map("hyspt")
}
