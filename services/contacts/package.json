{
  "name": "@onecore/contacts",
  "version": "1.5.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "",
  "packageManager": "pnpm@10.28.1",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./build/esm/index.d.ts",
      "require": "./build/cjs/index.cjs",
      "import": "./build/esm/index.js"
    },
    "./swagger.json": "./build/swagger.json",
    "./domain": {
      "types": "./build/esm/domain/index.d.ts"
    },
    "./schema": {
      "types": "./build/esm/schema.d.ts",
      "require": "./build/cjs/schema.js",
      "import": "./build/esm/schema.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=production node -r dotenv/config --import tsx --watch src/index",
    "dev:local": "NODE_ENV=production DOTENV_CONFIG_PATH=.env.local node -r dotenv/config --import tsx --watch src/index",
    "dev:init": "pnpm prepare:dotenv",
    "dev:apply-seed": "pnpm test:create-db && ./scripts/apply-seed.sh",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare:dotenv": "if not exist .env copy .env.template .env",
    "prettier": "prettier --check ./src",
    "start": "node -r dotenv/config build/esm/index",
    "test": "pnpm test:create-db && DOTENV_CONFIG_PATH=.env.test jest --detectOpenHandles",
    "test:create-db": "./scripts/create-test-db.sh",
    "test:ci": "pnpm test:create-db && DOTENV_CONFIG_PATH=.env.ci jest",
    "test:watch": "DOTENV_CONFIG_PATH=.env.test jest --watch",
    "ts:watch": "tsc --watch --noEmit",
    "typecheck": "tsc -p ./tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "@iteam/config": "^12.1.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@onecore/utilities": "workspace:*",
    "@types/swagger-ui": "^3.52.4",
    "dotenv": "^16.4.5",
    "knex": "^3.1.0",
    "koa": "^2.16.2",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-okapi-router": "^0.1.4",
    "koa-pino-logger": "^4.0.0",
    "koa2-swagger-ui": "^5.11.0",
    "mssql": "^11.0.1",
    "prettier": "^3.6.2",
    "tedious": "^18.6.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "9.32.0",
    "@types/jest": "^29.5.14",
    "@types/koa": "^2.15.0",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/mssql": "^9.1.8",
    "@types/node": "^20.11.5",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/typescript-estree": "^8.6.0",
    "axios": "^1.9.0",
    "eslint": "^9.31.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2"
  }
}
