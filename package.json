{
  "name": "onecore-monorepo",
  "version": "1.4.22",
  "private": true,
  "author": "Bostads AB Mimer",
  "packageManager": "pnpm@10.16.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "pnpm -r build",
    "build:apps": "pnpm run build:internal-portal && pnpm run build:property-tree",
    "build:communication": "pnpm run build --filter @onecore/communication",
    "build:core": "pnpm run build --filter @onecore/core",
    "build:economy": "pnpm run build --filter @onecore/economy",
    "build:internal-portal": "pnpm run build --filter @onecore/internal-portal && pnpm run build:internal-portal-backend",
    "build:internal-portal-backend": "pnpm run build --filter @onecore/internal-portal-backend",
    "build:leasing": "pnpm run build --filter @onecore/leasing",
    "build:libs": "pnpm run build:types && pnpm run build:utilities",
    "build:property": "pnpm run build --filter @onecore/property",
    "build:property-management": "pnpm run build --filter @onecore/property-management",
    "build:property-tree": "pnpm run build --filter @onecore/property-tree",
    "build:services": "pnpm run build:leasing && pnpm run build:economy && pnpm run build:property-management && pnpm run build:communication && pnpm run build:work-order && pnpm run build:property",
    "build:types": "pnpm run build --filter @onecore/types",
    "build:utilities": "pnpm run build --filter @onecore/utilities",
    "build:work-order": "pnpm run build --filter @onecore/work-order",
    "db:init": "./scripts/db/initialize.sh",
    "dev:init": "pnpm run git:configure && pnpm run db:init && pnpm -r dev:init",
    "dev": "turbo run dev",
    "generate:static": "pnpm run generate:static --workspaces --if-present",
    "git:configure": "git config merge.semver.driver \"scripts/git/merge-semver.js %O %A %B\"",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "prettier": "pnpm -r run prettier",
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck"
  },
  "dependencies": {
    "@azure/msal-node": "^2.9.2"
  },
  "devDependencies": {
    "eslint": "^9.31.0",
    "@eslint/js": "9.32.0",
    "@iteam/eslint-config-react": "^0.4.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-testing-library": "^7.6.1",
    "globals": "^15.9.0",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "syncpack": "^13.0.4",
    "turbo": "^2.5.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "8.42.0"
  }
}
