{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "concurrency": "12",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist/**", "./build/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    }
  }
}
