{
  "name": "@onecore/utilities",
  "version": "1.5.0",
  "type": "commonjs",
  "description": "ONECore common utilities",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/Bostads-AB-Mimer/onecore-utilities#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bostads-AB-Mimer/onecore-utilities.git"
  },
  "bugs": {
    "url": "https://github.com/Bostads-AB-Mimer/onecore-utilities/issues"
  },
  "author": "Anders Norrback Bornholm",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier": "prettier --check ./src",
    "test": "jest",
    "test:ci": "jest --silent",
    "test:watch": "jest --watch",
    "ts:watch": "tsc --watch --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@elastic/transport": "^8.0.0",
    "@elastic/elasticsearch": "^8.0.0",
    "exceljs": "^4.4.0",
    "knex": "^3.1.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "koa": "^2.16.2",
    "pino": "^9.1.0",
    "pino-elasticsearch": "^8.0.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.6.2",
    "release-please": "^16.8.0"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/jest": "^29.5.14",
    "@types/koa": "^2.15.0",
    "exceljs": "^4.4.0",
    "knex": "^3.1.0",
    "@types/pino-multi-stream": "^5.1.6",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.6.2"
  }
}
