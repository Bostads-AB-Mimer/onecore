{
  "name": "@onecore/core",
  "version": "1.5.0",
  "private": "true",
  "description": "",
  "license": "MIT",
  "author": "",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -e ts,js --exec pnpm start",
    "dev:init": "pnpm run prepare:dotenv",
    "generate-types": "pnpm generate-types:property-base & pnpm generate-types:work-order",
    "generate-types:property-base": "npx openapi-typescript http://localhost:5050/swagger.json -o ./src/adapters/property-base-adapter/generated/api-types.ts",
    "generate-types:work-order": "npx openapi-typescript http://localhost:5070/swagger.json -o ./src/adapters/work-order-adapter/generated/api-types.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prepare:dotenv": "[ -f .env ] || cp .env.template .env",
    "prettier": "prettier --check ./src",
    "dev:script:handle-expired-offers": "ts-node src/scripts/handle-expired-offers.ts",
    "dev:script:start-offer-batches": "ts-node src/scripts/start-offer-batches.ts",
    "script:handle-expired-offers": "node build/scripts/handle-expired-offers",
    "script:start-offer-batches": "node build/scripts/start-offer-batches",
    "start": "ts-node ./src/index.ts",
    "test": "jest",
    "test:ci": "TZ=UTC jest --silent",
    "test:watch": "TZ=UTC jest --watch",
    "ts:watch": "tsc --watch --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iteam/config": "^12.1.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@onecore/types": "workspace:*",
    "@onecore/utilities": "workspace:*",
    "@types/lodash": "^4.17.13",
    "axios": "^1.9.0",
    "axios-logger": "^2.8.0",
    "date-holidays": "^3.23.12",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.5",
    "http-errors": "^2.0.0",
    "jose": "^5.10.0",
    "jsonwebtoken": "^9.0.2",
    "keycloak-koa": "1.3.1",
    "koa": "^2.16.2",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-pino-logger": "^4.0.0",
    "koa2-swagger-ui": "^5.11.0",
    "lodash": "^4.17.21",
    "odoo-await": "^3.4.1",
    "openapi-fetch": "^0.13.5",
    "pino": "^9.1.0",
    "pino-elasticsearch": "^8.0.0",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^11.1.0",
    "striptags": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-pino-logger": "3.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "12.0.4",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.11.5",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui": "^3.52.4",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-node": "^11.1.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "msw": "^2.7.3",
    "nock": "^13.5.4",
    "nodemon": "^3.1.7",
    "openapi-typescript": "^6.7.6",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "8.42.0"
  }
}
